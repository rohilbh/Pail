<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rohilbh.pail" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_display_category" android:name="com.example.rohilbh.pail.DisplayCategoryActivity" android:parentActivityName="com.example.rohilbh.pail.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.example.rohilbh.pail.MainActivity"/>
        </activity>
    </application>

</manifest>

